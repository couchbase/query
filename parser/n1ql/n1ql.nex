/[wW][hH][eE][rR][eE]/           { yylex.logToken(yylex.Text(), "WHERE"); return WHERE }
/[wW][hH][iI][lL][eE]/           { yylex.logToken(yylex.Text(), "WHILE"); return WHILE }
